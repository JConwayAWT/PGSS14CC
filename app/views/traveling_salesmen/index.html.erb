<h1>Listing traveling_salesmen</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @traveling_salesmen.each do |traveling_salesman| %>
      <tr>
        <td><%= link_to 'Show', traveling_salesman %></td>
        <td><%= link_to 'Edit', edit_traveling_salesman_path(traveling_salesman) %></td>
        <td><%= link_to 'Destroy', traveling_salesman, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Traveling salesman', new_traveling_salesman_path %>
